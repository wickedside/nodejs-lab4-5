{
  "name": "basic-nodejs-mitso",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "module",
  "description": "nodejs-mitso",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "build": "tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "lint": "eslint 'src/**/*.ts' --fix",
    "lint-staged": "lint-staged"
  },
  "keywords": [
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0 <19"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.11",
    "@types/uuid": "^9.0.8",
    "debug": "^4.3.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "typescript": "^5.4.5"
  }
}
